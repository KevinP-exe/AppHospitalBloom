<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_eliminar">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="341dp"
        android:layout_height="59dp"
        android:text="Esta apuento de borrar un paciente del sistema esta seguro/a de esto?"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="347dp" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="181dp"
        android:layout_height="310dp"
        android:layout_marginTop="108dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_2"
        tools:layout_editor_absoluteX="-63dp" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="83dp"
        android:layout_height="313dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_15" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="145dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_11"
        tools:layout_editor_absoluteX="135dp" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/img_16"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="171dp" />

    <Button
        android:id="@+id/btn_volver"
        android:layout_width="128dp"
        android:layout_height="72dp"
        android:layout_marginStart="37dp"
        android:layout_marginBottom="172dp"
        android:backgroundTint="#85ADD4"
        android:text="Volver"
        android:textColorLink="#166FC180"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_eliminaar"
        android:layout_width="139dp"
        android:layout_height="67dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="177dp"
        android:backgroundTint="#E53935"
        android:text="Eliminar"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>